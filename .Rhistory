// [[Rcpp::plugins(cpp11)]]
#include <Rcpp.h>
#include <unordered_set>
using namespace Rcpp;
// [[Rcpp::export]]
NumericVector uniqueC(NumericVector x) {
std::unordered_set<int> seen;
int n = x.size();
LogicalVector dup(n);
std::vector<double> out;
for (int i = 0; i < n; ++i) {
dup[i] = !seen.insert(x[i]).second;
if (!dup[i]) {
out.push_back(x[i]);
}
}
return wrap(out);
}
